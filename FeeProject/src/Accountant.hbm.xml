<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="AccountantBean" table="FEE_ACCOUNTANT">
      <id name="id" type="integer" column="ID">
          <generator class="assigned"></generator>
      </id>
      <property name="name" type="string" column="name"></property>
      <property name="email" type="string" column="email"></property>
      <property name="password" type="string" column="password"></property>
         <property name="address" type="string" column="address"></property>
          <property name="contact" type="string" column="contact"></property>
  </class>
</hibernate-mapping>